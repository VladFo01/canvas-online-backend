{
    "env": {
        "commonjs": true,
        "node": true,
        "es2022": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "class-methods-use-this": 0,
        "no-use-before-define": 0
    }
}